pipeline {
    agent any
    stages {
        stage('Clean') {
        	steps {
	           	bat 'Section-04c-01_JavaTutorial_MavenProject\\mvn clean' 
	        }
           
        }
        stage('Test') {
        	steps {
	            bat 'mvn test' 
	        }
          
        }
        stage('Package') {
        	steps {
	            bat 'mvn package'
	        }
           
        }
    }
}